{
  "name": "place",
  "version": "0.0.1",
  "description": "Place analysis",
  "main": "index.js",
  "scripts": {
    "start": "node dev-server.js",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/conveyal/place.git"
  },
  "keywords": [
    "place",
    "transit"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/place/issues"
  },
  "homepage": "https://github.com/conveyal/place",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "express": "^4.13.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "css-loader": "^0.20.2",
    "elemental": "^0.5.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "history": "^1.12.5",
    "leaflet": "^0.7.5",
    "moment": "^2.10.6",
    "postcss-loader": "^0.7.0",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-leaflet": "^0.8.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "xhr": "^2.1.0"
  },
  "babel": {
    "stage": 0,
    "env": {
      "development": {
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": [
                  "react",
                  "redbox-react"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
