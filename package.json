{
  "name": "@conveyal/taui",
  "description": "Isochrone & accessibility analysis",
  "main": "src/index.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "next build",
    "check-format": "prettier -l \"src/**/*.js\" \"pages/**/*.js\"",
    "dev": "next",
    "format": "prettier --write \"src/**/*.js\" \"pages/**/*.js\"",
    "now-build": "next build",
    "predev": "yarn",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/taui.git"
  },
  "keywords": [
    "taui",
    "transit"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/taui/issues"
  },
  "homepage": "https://github.com/conveyal/taui",
  "dependencies": {
    "@conveyal/lonlat": "^1.4.0",
    "@conveyal/woonerf": "^4.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mapbox/polyline": "^1.1.0",
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "js-cookie": "^2.2.0",
    "jsolines": "^2.1.1",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.1.1",
    "memoize-one": "^5.0.5",
    "next": "v9.0.2",
    "next-cookies": "^1.1.2",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "react": "v16.8.6",
    "react-dom": "v16.8.6",
    "react-modal": "^3.9.1",
    "react-redux": "^7.1.0",
    "react-select": "^3.0.4",
    "react-select-fast-filter-options": "^0.2.3",
    "react-virtualized-select": "^3.1.3",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "regenerator-runtime": "^0.13.3",
    "semantic-release": "^15.13.18"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": false
  }
}
